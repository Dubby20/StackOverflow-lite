{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "StackOverflow-lite is a platform where people can ask questions and provide answers",
  "main": "server.js",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --recursive ./test/",
    "mocha": "mocha --compilers js:babel-register",
    "babel-node": "babel-node --presets=env",
    "start": "nodemon --exec npm run babel-node -- server.js",
    "build": "babel src -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dubby20/StackOverflow-lite.git"
  },
  "keywords": [],
